FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/constant/caseSettings"

dimensions      [0 2 -2 0 0 0 0];

internalField   $:initial.p;

boundaryField
{
    inlet  { $:inlet.p; }

    outlet { $:outlet.p }

    wall   { $:wall.p }
}

