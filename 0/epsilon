FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/constant/caseSettings"

dimensions      [0 2 -3 0 0 0 0];

internalField   $:initial.epsilon;

boundaryField
{
    inlet { $:inlet.epsilon; }

    outlet { $:outlet.epsilon; }

    wall   { $:wall.epsilon; }
}


